{
  "files.associations": {
    "*.func": "shellscript"
  },
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
  },
  "shellcheck.customArgs": [
    "--external-sources",
  ],
  "shellcheck.useWorkspaceRootAsCwd": true,
  "shellformat.useEditorConfig": true,
}
